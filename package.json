{
	"name": "embed-archive-org",
	"version": "0.1.0",
	"description": "Embed content from Archive.org.",
	"author": "mediaformat",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build --webpack-copy-php",
		"format": "npm run format:js && npm run format:php",
		"format:js": "wp-scripts format",
		"format:php": "composer run format",
		"lint": "npm run lint:js && npm run lint:php",
		"lint:js": "wp-scripts lint-js",
		"lint:php": "composer run lint",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip --root-folder='embed-archive-org'",
		"start": "wp-scripts start --webpack-copy-php",
		"env": "wp-env"
	},
	"files": [
		"plugin.php",
		"build",
		"vendor/autoload.php",
		"vendor/composer"
	],
	"dependencies": {
		"@wordpress/blocks": "^15.3.0",
		"@wordpress/i18n": "^6.3.0",
		"@wordpress/primitives": "^4.30.0",
		"compose": "^0.1.2"
	},
	"devDependencies": {
		"@wordpress/eslint-plugin": "^22.16.0",
		"@wordpress/scripts": "^30.23.0"
	}
}
